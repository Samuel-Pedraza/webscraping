<div class="container">
	<table class="table table-striped table-hover">
		<thead class="thead-inverse">
			<td>Product Key</td>
			<td>Price</td>
		</thead>
		<% if @returnedarray %>
			<% @returnedarray.each_with_index do |item, index| %>
				<% if index % 2 == 0 %>
				<tr>
					<td><%= item %></td>
					<% elsif index % 2 == 1 %>
					<td>$<%= item %></td>
				</tr>				
				<% end %>
			<% end %>
		<% end %>
	</table>
</div>